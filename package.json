{
  "name": "polcompballvalues",
  "version": "2.2.8",
  "devDependencies": {
    "coffeescript": "^2.7.0",
    "cson": "^7.20.0",
    "pug": "^3.0.2",
    "sass": "^1.58.1",
    "terser": "^5.16.3"
  },
  "scripts": {
    "build": "coffee ./scripts/build.coffee && coffee -c -b -m -o ./dist ./src/coffeescript && sass ./src/sass/style.sass ./dist/style.css",
    "coffeescript_watch": "coffee -c -b -m -w -o ./dist ./src/coffeescript",
    "sass_watch": "sass ./src/sass/style.sass ./dist/style.css -w",
    "minify": "coffee ./scripts/minify.coffee",
    "release": "coffee ./scripts/build.coffee && coffee -c -b -m -o ./dist ./src/coffeescript && sass ./src/sass/style.sass ./dist/style.css --style compressed && coffee ./scripts/minify.coffee"
  }
}
